{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "apidemo": {
        "Protocols": "TCP",
        "Match": {
          "Hosts": [ "*:5100" ]
        },
        "ClusterId": "apidemo",
        "RetryCount": 1,
        "Timeout": "00:00:11"
      },
      "udpTest": {
        "Protocols": [ "TCP", "UDP" ],
        "Match": {
          "Hosts": [ "*:5000" ]
        },
        "ClusterId": "udpTest",
        "RetryCount": 1,
        "UdpResponses": 1,
        "Timeout": "00:00:11"
      }
    },
    "Clusters": {
      "udpTest": {
        "LoadBalancingPolicy": "RoundRobin",
        "HealthCheck": {
          "Passive": {
            "Enable": true
          }
        },
        "Destinations": [
          {
            "Address": "127.0.0.1:11000"
          }
        ]
      },
      "apidemo": {
        "LoadBalancingPolicy": "RoundRobin",
        "HealthCheck": {
          "Active": {
            "Enable": false,
            "Policy": "Connect"
          }
        },
        "Destinations": [
          {
            "Address": "[::1]:5144"
          },
          {
            "Address": "[::1]:5146"
          },
          {
            "Address": "google.com:998"
          },
          {
            "Address": "www.baidu.com"
          },
          {
            "Address": "http://google.com"
          },
          {
            "Address": "https://google.com"
          }
        ]
      }
    }
  }
}