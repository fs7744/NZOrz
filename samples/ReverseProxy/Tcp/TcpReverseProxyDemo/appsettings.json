{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "apidemo": {
        "Protocols": "TCP",
        "Match": {
          "Hosts": [ "*:5000" ]
        },
        "ClusterId": "apidemo",
        "RetryCount": 1
      }
    },
    "Clusters": {
      "apidemo": {
        "LoadBalancingPolicy": "PowerOfTwoChoices",
        "HealthCheck": {
          "Active": {
            "Policy": "Tcp"
          }
        },
        "Destinations": [
          {
            "Address": "[::1]:5144"
          },
          {
            "Address": "[::1]:5146"
          },
          {
            "Address": "google.com:998"
          },
          {
            "Address": "google.com"
          },
          {
            "Address": "http://google.com"
          },
          {
            "Address": "https://google.com"
          }
        ]
      }
    }
  }
}